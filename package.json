{
  "name": "blog.ews",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/miao17game/ews-blog.git",
  "contributors": [
    "bigmogician <bigmogician@outlook.com>"
  ],
  "license": "Apache License 2.0",
  "private": true,
  "scripts": {
    "install:client": "cd client && yarn install",
    "install:server": "cd server && yarn install",
    "install:all": "yarn install:client && yarn install:server",
    "start": "cd server && yarn start --watch",
    "start:fix": "cd server && yarn start:fix",
    "start:prod": "cd server && yarn start:prod",
    "dev": "cd client && yarn start",
    "build:server": "cd server && yarn build",
    "build:client": "cd client && yarn build",
    "build": "yarn build:client && yarn build:server",
    "postbuild": "npx rimraf server/build && npx copy client/dist/**/*.* server/build",
    "docker:build": "node scripts/docker.js"
  },
  "dependencies": {},
  "devDependencies": {
    "copy": "^0.3.2",
    "rimraf": "^3.0.0"
  }
}
